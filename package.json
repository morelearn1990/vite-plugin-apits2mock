{
  "name": "vite-plugin-apits2mock",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@8.15.3",
  "description": "request mock for swagger2ts",
  "author": "morelearn1990 <morelearn1990@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/morelearn1990",
  "homepage": "https://github.com/morelearn1990/vite-plugin-apits2mock#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morelearn1990/vite-plugin-apits2mock.git"
  },
  "bugs": "https://github.com/morelearn1990/vite-plugin-apits2mock/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch --ignore-watch examples",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "fast-glob": "^3.3.2",
    "micromatch": "^4.0.5",
    "mockjs": "^1.1.0",
    "path-to-regexp": "^6.2.1",
    "ts-json-schema-generator": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/json-schema": "^7.0.15",
    "@types/micromatch": "^4.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.11.19",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.3",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
